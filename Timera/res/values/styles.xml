<resources>

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

    <style name="PickButtonA">
        <item name="android:background">@drawable/main_pick_a</item>
    </style>

    <style name="PickButtonB">
        <item name="android:background">@drawable/main_pick_b</item>
    </style>

    <style name="PickButtonGallery">
        <item name="android:background">@drawable/pick_btn_gallery</item>
    </style>

    <style name="PickButtonCamera">
        <item name="android:background">@drawable/pick_btn_camera</item>
    </style>

    <style name="PickButtonFlickr">
        <item name="android:background">@drawable/pick_btn_flickr</item>
    </style>

    <style name="PickButtonNearby">
        <item name="android:background">@drawable/pick_btn_nearby</item>
    </style>

    <style name="MixButtonConfirm">
        <item name="android:background">@drawable/mix_btn_confirm</item>
    </style>

    <style name="MixButtonBack">
        <item name="android:background">@drawable/mix_btn_back</item>
    </style>

    <style name="ResultButtonSave">
        <item name="android:background">@drawable/result_btn_save</item>
    </style>

    <style name="ResultButtonFlickr">
        <item name="android:background">@drawable/result_btn_flickr</item>
    </style>

</resources>